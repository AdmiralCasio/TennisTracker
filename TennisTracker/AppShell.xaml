<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="TennisTracker.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:TennisTracker"
    Shell.FlyoutBehavior="Flyout"
    Title="TennisTracker">


        <TabBar>
            <ShellContent
                Title="Tennis Tracker"
                ContentTemplate="{DataTemplate local:MainPage}"
                Route="MainPage" />

            <ShellContent
                Title="New Match"
                ContentTemplate="{DataTemplate local:NewMatch}"
                Route="NewMatch" />
        </TabBar>
    
</Shell>
